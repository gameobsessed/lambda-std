{
  "name": "lambda-std",
  "description": "Lambda Simple Toolkit based on Decorators",
  "version": "0.7.3",
  "private": false,
  "license": "MIT",
  "author": "Denis Zatsepin <denis@zatsepin.me>",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -d",
    "test": "jest",
    "lint": "eslint --cache --fix",
    "prepublish": "npm run lint && npm run build"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gameobsessed/lambda-std.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "decorators",
    "typescript",
    "framework",
    "std",
    "aws-sdk-v3"
  ],
  "dependencies": {
    "class-transformer": "^0.5.0",
    "source-map-support": "^0.5.16",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@commitlint/top-level": "11.0.0",
    "@types/aws-lambda": "8.10.85",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.9",
    "@types/uuid": "8.3.3",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "5.0.0",
    "husky": "5.2.0",
    "jest": "27.3.1",
    "joi": "17.4.2",
    "lint-staged": "10.5.4",
    "prettier": "2.4.1",
    "reflect-metadata": "0.1.13",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,html,json}": "yarn prettier --write",
    "src/**/*.{ts}": "yarn eslint --cache --fix"
  }
}
